<h1>Wikis</h1>

<div class="row">
    <div class="col-md-8">
        <div class="col-md-16">
        </div>
            <% (@wikis).each do |wiki| %>
                <div class="media">
                    <div class="media-body">
                        <h4 class="media-housing"> <%= link_to wiki.title, wiki %></h4>
                 </div>
            </div>
        <% end %>
    </div>
</div>