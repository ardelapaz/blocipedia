<div class="row">
<div class="col-md-4">
          <%= link_to "Edit Wiki", edit_wiki_url, class: 'btn btn-success' %>
          <%= link_to "Delete Wiki", [@wiki], method: :delete, class: 'btn btn-danger', data: { confirm: "Are you sure you want to delete this wiki?" } %>
    </div>
    <div class="col-md-12">
        <h1><%= @wiki.title %></h1>
            <div class="row">
                <div class="col-md-8">
                    <p><%= markdown(@wiki.body) %></p>
                </div>
            </div>
    </div>
</div>
